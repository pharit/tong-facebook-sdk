{
    "name": "pharit/tong-facebook-sdk",
    "description": "A comprehensive Facebook SDK API package for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/config": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Tong\\FacebookSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tong\\FacebookSdk\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tong\\FacebookSdk\\FacebookSdkServiceProvider"
            ],
            "aliases": {
                "Facebook": "Tong\\FacebookSdk\\Facades\\Facebook"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
